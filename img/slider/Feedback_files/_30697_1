  <script type="text/javascript">

        //DO NOT USE JQUERY, IT WILL MESS UP BLACKBOARDS LIBRARY
        (function() { //DOCUMENT ON READY
            //document.getElementById("redirectProvidersDropdownButton").innerText = "Acceder con el correo institucional";

            window.onresize = function() {
                toggleSize();
            }

            document.getElementById("loginFormList").innerHTML += " <li class='clearfix text-center'> <a style='text-align: center !important;' href='http://uacademy.unibe.edu.do/renovacionIntranet/recclaacc.aspx'>Olvidé mi constraseña</a></li>";

            document.getElementById("loginErrorMessage").innerHTML = `El nombre de usuario o la contraseña que escribió son incorrectos. Inténtelo nuevamente. Si aún no puede iniciar sesión, comuníquese con nuestra mesa de ayuda al 809.689.4111 x-1178 o enviando correo a help@unibe.edu.do`; 
        })();

        let toggle = 0;
        let navlinks = document.getElementsByClassName("nav-link");

        function loadLoginPage() {
            if(document.forms.login.user_id != undefined)
            {
                verify_username_password(document.forms.login);
            }
            setTimeout("triggerScreenreaderAlert()", 500);
        }

        function triggerScreenreaderAlert() {
            if (document.getElementById('loginErrorMessage')) {
                $('loginErrorMessage').update($('loginErrorMessage').innerHTML);
            }
        }

        document.getElementById("toggleBtnMenu").addEventListener("click", function(){
            if(toggle === 0){
                for(var i = 0; i < navlinks.length; i++){
                    navlinks[i].style.display = "block";
                }
                document.getElementsByClassName("nav")[0].style.height="auto";
                document.getElementById('lang-select-button').style.display = 'none';
                toggle = 1;
            }else{
                for(var i = 0; i < navlinks.length; i++){
                    navlinks[i].style.display = "none";
                }
                document.getElementsByClassName("nav")[0].style.height="64px";
                document.getElementById('lang-select-button').style.display = 'block';
                toggle = 0;
            }
        });

        function toggleSize() {

            if (window.innerWidth > 620) {
                for(var i = 0; i < navlinks.length; i++){
                    navlinks[i].style.display = "inline-block";
                }
                document.getElementsByClassName("nav")[0].style.height="64px";
                document.getElementById('lang-select-button').style.display = 'block';
                toggle = 0;
            }

            else{
                for(var i = 0; i < navlinks.length; i++){
                    navlinks[i].style.display = "none";
                }
                document.getElementsByClassName("nav")[0].style.height="64px";
                document.getElementById('lang-select-button').style.display = 'block';
                toggle = 0;
            }
        }

    </script>

  <script type="text/javascript">
